{
    "compilerOptions": {
        "allowJs": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "sourceMap": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "build",
        "moduleResolution": "Node",
        "module": "CommonJS",
        "target": "ES2022",
    },
}

